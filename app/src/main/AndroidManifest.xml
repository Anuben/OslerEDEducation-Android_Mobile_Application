<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.oslerededucation">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.OslerEDEducation">
        <activity
            android:name=".MainActivity"
            android:theme="@style/Theme.OslerEDEducation.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".RotationContacts"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".ParkingBadgeITCredentials"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".HomePage"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".SecondMap"
            android:parentActivityName=".DepartmentMap" />
        <activity
            android:name=".FirstMap"
            android:parentActivityName=".DepartmentMap" />
        <activity
            android:name=".EGHContacts"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".BCHContacts"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".EducationalResources"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".DepartmentMap"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".BchDepartmentMap"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".COVID19Screening"
            android:parentActivityName=".MainActivity" />


        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>